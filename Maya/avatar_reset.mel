$blendShapes = "ExpressionBlendshapes";
string $blendShapesArray[] = `listAttr -m ($blendShapes + ".w")`;
int $s = `size($blendShapesArray)`;
int $i;
for($i = 0; $i < $s; $i++) {
    string $target = "ExpressionBlendshapes." + $blendShapesArray[$i];
    setAttr $target 0;
    print("reset " + $target + "\n");
}